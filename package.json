{
  "name": "react-use-external-assets",
  "version": "1.0.0",
  "description": "Append multiple scripts and stylesheets to the document body as functions",
  "author": "wimoMisterX <wimo@wimodya-athukorala.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "prettier": "prettier \"{.,src,src/**,example/src,example/src/**,types}/*.{md,js,jsx,ts,tsx,json}\""
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@changesets/cli": "^2.22.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "cross-env": "^5.2.0",
    "eslint": "7.x",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "2.2.1",
    "react": "^16.9.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^2.26.9",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wimoMisterX/react-use-external-assets.git"
  },
  "bugs": {
    "url": "https://github.com/wimoMisterX/react-use-external-assets/issues"
  },
  "keywords": [
    "react",
    "component",
    "external",
    "scripts",
    "hooks"
  ],
  "homepage": "https://github.com/wimoMisterX/react-use-external-assets#readme"
}
